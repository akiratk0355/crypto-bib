@inproceedings{euro/chs14,
  abstract = {We describe an implementation of fast elliptic curve scalar multiplication, optimized for Diffie\textendash{}Hellman Key Exchange at the 128-bit security level. The algorithms are compact (using only x-coordinates), run in constant time with uniform execution patterns, and do not distinguish between the curve and its quadratic twist; they thus have a built-in measure of side-channel resistance. (For comparison, we also implement two faster but non-constant-time algorithms.) The core of our construction is a suite of two-dimensional differential addition chains driven by efficient endomorphism decompositions, built on curves selected from a family of {$\mathbb{Q}$}-curve reductions over {$\mathbb{F}$}p2Fp2\textbackslash{}mathbb\{F\}\_\{p\^2\} with p = 2127 - 1. We include state-of-the-art experimental results for twist-secure, constant-time, x-coordinate-only scalar multiplication.},
  author = {Costello, Craig and Hisil, Huseyin and Smith, Benjamin},
  booktitle = {{{EUROCRYPT}} 2014},
  doi = {10.1007/978-3-642-55220-5_11},
  isbn = {978-3-642-55219-9 978-3-642-55220-5},
