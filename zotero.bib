  language = {en},
  pages = {169-189},
  publisher = {{Springer Berlin Heidelberg}},
  series = {Lecture {{Notes}} in {{Computer Science}}},
  title = {Progression-{{Free Sets}} and {{Sublinear Pairing}}-{{Based Non}}-{{Interactive Zero}}-{{Knowledge Arguments}}},
  year = {2012}
}

